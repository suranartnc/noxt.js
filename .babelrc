{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    "transform-decorators-legacy",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "noxt": "./.noxt/",
        "components": "./src/app/components",
        "containers": "./src/app/containers",
        "pages": "./src/app/pages",
        "utils": "./src/app/utils",
        "styles": "./src/app/styles"
      }
    }]
  ]
}
